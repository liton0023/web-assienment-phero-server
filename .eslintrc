{
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [],
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-console": "off",
        "no-undef": "error",
        "no-unused-expressions": "error",
        "no-unreachable": "error"
    },
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "process": "readonly"
    }
}